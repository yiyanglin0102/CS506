hods: {\n    loginClick: function() {\n      firebase\n        .auth()\n        .signInWithEmailAndPassword(this.email, this.password)\n        .then(\n          () => {\n            // alert(\"You are signed in!\");\n            this.$router.replace(\"home\");\n          },\n          err => {\n            alert(\"Oops. \" + err.message);\n          }\n        );\n    },\n    loginEnter: function(e) {\n      if (e.keyCode === 13) {\n        // alert(\"Enter was pressed\");\n        firebase\n          .auth()\n          .signInWithEmailAndPassword(this.email, this.password)\n          .then(\n            () => {\n              // alert(\"You are signed in!\");\n              this.$router.replace(\"home\");\n            },\n            err => {\n              alert(\"Oops. \" + err.message);\n            }\n          );\n      }\n    },\n    googleLogin: function() {\n      firebase\n        .auth()\n        .signInWithPopup(provider)\n        .then(\n          () => {\n            this.$router.replace(\"home\");\n          },\n          err => {\n            alert(\"Oops. \" + err.message);\n          }\n        );\n    }\n  }\n};\n</script>\n\n<style scoped>\n.login {\n  margin-top: 40px;\n}\ninput {\n  margin: 10px 0;\n  width: 20%;\n  padding: 15px;\n}\nbutton {\n  margin-top: 20px;\n  width: 10%;\n  cursor: pointer;\n}\np {\n  margin-top: 40px;\n  font-size: 13px;\n}\np a {\n  text-decoration: underline;\n  cursor: pointer;\n}\n</style>\n"]}]}