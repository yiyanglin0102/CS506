AA,CAAA,SAAA,GAAA,GAAA,CAAA,IAAA,GAAA,SAAA;AACA,UAAA,EAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,GAAA,WAAA;AACA,UAAA,EAAA,CAAA,gBAAA,GAAA,GAAA,CAAA,IAAA,GAAA,gBAAA,CARA,CAUA;AACA,SAXA;AAYA,OAjBA;AAkBA,KAtBA;AAuBA,IAAA,MAAA,EAAA,kBAAA;AAAA;;AACA,MAAA,QAAA,CACA,IADA,GAEA,OAFA,GAGA,IAHA,CAGA,YAAA;AACA,QAAA,KAAA,CAAA,OAAA,CAAA,OAAA,CAAA,OAAA;AACA,OALA;AAMA,KA9BA;AA+BA,IAAA,OAAA,EAAA,mBAAA;AACA,UAAA,MAAA,GAAA,QAAA,CACA,SADA,GAEA,UAFA,CAEA,OAFA,EAGA,GAHA,CAGA,sBAHA,CAAA;AAIA,MAAA,MAAA,CACA,GADA,GAEA,IAFA,CAEA,UAAA,GAAA,EAAA;AACA,YAAA,GAAA,CAAA,MAAA,EAAA;AACA,iBAAA,GAAA,CAAA,IAAA,GAAA,KAAA;AACA,SAFA,MAEA;AACA,UAAA,OAAA,CAAA,GAAA,CAAA,OAAA;AACA;AACA,OARA,WASA,UAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,UAAA,EAAA,KAAA;AACA,OAXA;AAYA,KAhDA;AAiDA,IAAA,gBAjDA,8BAiDA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,MAAA,QAAA,CACA,SADA,GAEA,UAFA,CAEA,OAFA,EAGA,GAHA,GAIA,IAJA,CAIA,UAAA,aAAA,EAAA;AACA,QAAA,aAAA,CAAA,OAAA,CAAA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,GAAA,OAAA,IAAA,EAAA,EAAA;AACA,YAAA,EAAA,CAAA,KAAA,CAAA,IAAA,CAAA,GAAA,CAAA,IAAA,EAAA;AACA,YAAA,EAAA,CAAA,KAAA,CAAA,IAAA;AACA;AACA,SALA;AAMA,OAXA;AAYA;AA/DA,GAfA;AAgFA,EAAA,OAhFA,qBAgFA;AACA,SAAA,KAAA;AACA,SAAA,gBAAA;AACA;AAnFA,CAAA","sourcesContent":["  \n<template>\n  <span class=\"table table-hover table-dark\">\n    <div class=\"hi\">\n      <h1 class=\"p-3 mb-2 bg-info text-white\">\n        <p>Welcome to My Bulletin Board</p>\n      </h1>\n      <router-link class=\"badge badge-primary text-wrap\" style=\"width: 6rem;\" to=\"/Login\">Login</router-link>\n      <button @click=\"logout\" class=\"badge badge-primary text-wrap\" style=\"width: 6rem;\">Logout</button>\n\n      <br />\n      <router-link to=\"/home\">Back to Personal Profile</router-link>\n      <br />\n\n      <table class=\"table table-hover table-dark\" >\n        <thead>\n          <tr>\n            <th scope=\"col\" >First Name</th>\n            <th scope=\"col\">Last Name</th>\n            <th scope=\"col\">Email</th>\n             <th scope=\"col\">Future Goals</th>\n                          <th scope=\"col\">Classes</th>\n\n            <th scope=\"col\">Message</th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"item in items\" :key=\"item.firstName\">\n            <td width=\"10%\">\n              <span v-text=\"item.firstName\"></span>\n            </td>\n            <td width=\"10%\">\n              <span v-text=\"item.lastName\"></span>\n            </td>\n            <td width=\"10%\">\n              <span v-text=\"item.email\"></span>\n            </td>\n              <td width=\"10%\">\n              <span v-text=\"item.futureGoals\"></span>\n            </td>\n            <td width=\"20%\">\n              <span v-text=\"item.myClasses\"></span>\n            </td>\n            <td width=\"30%\">\n              <span v-text=\"item.wantSay\"></span>\n            </td>\n          </tr>\n        </tbody>\n      </table>\n    </div>\n  </span>\n</template>\n\n\n<script>\nimport firebase from \"firebase\";\nexport default {\n  name: \"home\",\n  data() {\n    return {\n      items: [],\n      email: \"\",\n      password: \"\",\n      firstName: \"\",\n      lastName: \"\",\n      wantSay: \"\",\n      myClasses: \"\",\n      futureGoals: \"\",\n      interestingLinks: \"\"\n    };\n  },\n  methods: {\n    print: function() {\n      var vm = this;\n      // `${doc.id} => ${doc.data().email}`\n      firebase\n        .firestore()\n        .collection(\"users\")\n        .get()\n        .then(querySnapshot => {\n          querySnapshot.forEach(doc => {\n            // console.log(doc.data().name);\n            vm.firstName = doc.data().firstName;\n            vm.lastName = doc.data().lastName;\n            vm.email = doc.data().email;\n            vm.wantSay = doc.data().wantSay;\n            vm.myClasses = doc.data().myClasses;\n            vm.futureGoals = doc.data().futureGoals;\n            vm.interestingLinks = doc.data().interestingLinks;\n\n            // console.log(`${doc.id} => ${doc.data().wantSay}`);\n          });\n        });\n    },\n    logout: function() {\n      firebase\n        .auth()\n        .signOut()\n        .then(() => {\n          this.$router.replace(\"login\");\n        });\n    },\n    getdata: function() {\n      var docRef = firebase\n        .firestore()\n        .collection(\"users\")\n        .doc(\"gqPLrzAvZjwJYjFIMCzv\");\n      docRef\n        .get()\n        .then(function(doc) {\n          if (doc.exists) {\n            return doc.data().email;\n          } else {\n            console.log(\"找不到文件\");\n          }\n        })\n        .catch(function(error) {\n          console.log(\"提取文件時出錯:\", error);\n        });\n    },\n    fetchChatHistory() {\n      var vm = this;\n      firebase\n        .firestore()\n        .collection(\"users\")\n        .get()\n        .then(function(querySnapshot) {\n          querySnapshot.forEach(function(doc) {\n            if (doc.data().wantSay != \"\") {\n              vm.items.push(doc.data());\n              vm.items.sort();\n            }\n          });\n        });\n    }\n  },\n  created() {\n    this.print();\n    this.fetchChatHistory();\n  }\n};\n</script>\n\n<style scoped>\nbutton {\n  margin-top: 20px;\n  width: 10%;\n  cursor: pointer;\n  padding: 5px;\n  width: 30%;\n}\n.border {\n  border-style: dotted;\n  border-color: red;\n}\n</style>\n\n"],"sourceRoot":"src/views"}]}